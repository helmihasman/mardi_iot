extends base_datatables
block content 
    section.content-header 
        h1 
            | Report
            
        ol.breadcrumb 
            li 
                a(href='#') 
                    i.fa.fa-dashboard
                    | Report 
            
    section.content
        .box
            .box-header.with-border
                h3.box-title
                    | Report
                small.pull-right
                   a(href='/report') Back
                    
                        
            .box-body
                //- console.log("paaggee = "+JSON.stringify(data));
                //- console.log("paaggee = "+data[0].value.payload.tag_1.tag_id);
                table(id="example",class="table table-bordered table-striped")
                   thead
                     tr
                       th No
                       th Tag Id
                       th Category

                   tbody
                       if messages.msg_error
                                | !{messages.msg_error}
                            else
                                for row,index in data 
                                    tr
                                        td #{index+1}
                                        td #{row.id_linen}
                                        td #{row.linen_category}
                                        
                                            