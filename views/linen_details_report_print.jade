extends base_datatables_print
block content 
    section.content-header 
       
    section.content
        .box
                        
            .box-body
                h3 Linen for #{new_date}&nbsp&nbsp
                table(id="example",class="table table-bordered table-striped")
                   thead
                     tr
                       th(width='10%') No
                       th Linen ID
                       th Linen Category

                   tbody
                       if messages.msg_error
                                | !{messages.msg_error}
                            else
                                for row,index in data 
                                    tr
                                        td #{index+1}
                                        td #{row.linen_uuid}
                                        td #{row.linen_category}
                                        
                                            